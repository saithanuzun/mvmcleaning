<style>
    /* Custom styles for the carousel */
    .carousel-mobile-full {
        width: 100%;
        /* Full width on all screens by default */
        height: 100vh;
        /* Full viewport height on small screens */
        min-height: 450px;
        /* Ensure a minimum height even if vh isn't supported or is too small */
    }

    .carousel-mobile-full .carousel-item img {
        width: 100%;
        height: 100%;
        object-fit: cover;
    }

    /* Media query for larger screens (e.g., md and up) */
    @@media (min-width: 768px) {
        .carousel-mobile-full {
            width: 80%;
            /* 80% width on larger screens */
            height: 70vh;
            /* 70% viewport height on larger screens */
            max-width: 1200px;
            /* Optional: set a max width for very large screens */
            min-height: 500px;
            /* Ensure a good minimum height on larger screens */
        }
    }

    /* Ensure carousel-caption text is readable */
    .carousel-caption h1,
    .carousel-caption h4 {
        text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.7);
        /* Add text shadow for readability over images */
    }

    /* Adjust button size in carousel caption */
    .carousel-caption .btn {
        font-size: 1.1rem;
        /* Slightly larger font */
        padding: 0.8rem 1.8rem;
        /* Adjust padding */
    }

    @@media (min-width: 768px) {
        .carousel-caption .btn {
            font-size: 1.25rem;
            /* Larger font on desktop */
            padding: 1rem 2.5rem;
            /* More padding on desktop */
        }
    }
</style>

